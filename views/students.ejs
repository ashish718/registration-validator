<html>
<head>
<title>Student Details</title>

<link href="/stylesheets/student_style.css" rel="stylesheet">

</head>

<body>
<button id="formLink"> <a href='http://localhost:3000/'>Student Registration Form</a></button>
<h1>Student Details</h1>

<% if(result!=null) { %>
    <table id="students">
        <tr>
            <th> Name</th>
            <th>Roll No </th>
            <th>Class</th>
            <th>School </th>
            <th>Date Of Birth </th>
        </tr>
        <% result.forEach(function(item){ %>
        <tr>
            <td><%= item.name%></td>
            <td><%= item.rollno %></td>
            <td><%= item.class%></td>
            <td><%= item.school %></td>
            <td><%= item.dob%></td>

        </tr>
        <% }) %>
        <% if(result.length == 0) { %>
        <tr>
            <td>No Student Registered </td>
        </tr>
        <% } %>
    </table>
<% } %>


</body>
</html>